@article{7378457, 
author={C. Daversin, C. Prudhomme and C. Trophime}, 
journal={IEEE Transactions on Applied Superconductivity}, 
title={Full Three-Dimensional Multiphysics Model of High-Field Polyhelices Magnets}, 
year={2016}, 
volume={26}, 
number={4}, 
pages={1-4}, 
abstract={High-field resistive magnets for static field developed at the Laboratoire National des Champs Magnétiques Intenses are based on the so-called polyhelix technique. Their design relies on nonlinear 3-D multiphysics models. As the user demands for higher magnetic field or specific field profile are growing, we have to revisit our numerical models. They need to include more physics and more precise geometry. In this context, we have rewritten our numerical model in the frame of a collaboration with Institut de Recherche Mathématiques Avanc{\'e}s. New models have been implemented with the finite-element library Feel++. This paper gives a status of these developments and the new features available. Results are presented for a 14 polyhelices insert targeting 36 T in a 34-mm bore.}, 
keywords={finite element analysis;magnets;Institut de Recherche Mathématiques Avanc{\'e}e;Laboratoire National des Champs Magnétiques;finite-element library Feel++;geometry;high-field polyhelices magnet;high-field resistive magnet;magnetic flux density 36 T;nonlinear 3D multiphysics model;numerical model;size 34 mm;three-dimensional multiphysics model;Geometry;Magnetomechanical effects;Magnetostatics;Numerical models;Solid modeling;Superconducting magnets;Three-dimensional displays;Magnetic fields;Magnets;Multi-physics Analysis;magnets;multiphysics analysis}, 
doi={10.1109/TASC.2016.2516241}, 
ISSN={1051-8223}, 
month={June},
}
